{
  "compilerOptions": {
    "outDir": "dist",
    "jsx": "react",
    "lib": ["es5", "es6", "dom"],
    "module": "commonjs",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "target": "es5"
  },
  "exclude": [
    "node_modules",
    "*.spec.ts"
  ]
}
